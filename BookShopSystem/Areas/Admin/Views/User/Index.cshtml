
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_List.cshtml";
}
<div class="container-fluid am-cf">
    <div class="row">
        <div class="am-u-sm-12 am-u-md-12 am-u-lg-9">
            <div class="page-header-heading"><span class="am-icon-book page-header-heading-icon"></span> 会员管理 <small>Book</small></div>
        </div>
    </div>
</div>

<div class="row-content ">
    <div class="row" id="app-user">
        <div class="am-u-sm-12 am-u-md-12 am-u-lg-12">
            <div class="widget am-cf">
                <div class="widget-head am-cf">
                    <div class="widget-title  am-cf">会员列表</div>
                </div>

                <div class="am-u-sm-12 am-u-md-6 am-u-lg-6">
                    <div class="am-form-group">
                        <div class="am-btn-toolbar">
                            <div class="am-btn-group am-btn-group-xs">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="am-u-sm-12 am-u-md-12 am-u-lg-3">
                    <div class="am-form-group">
                    </div>
                </div>
                <div class="am-u-sm-12 am-u-md-12 am-u-lg-3">
                    <div class="am-input-group am-input-group-sm tpl-form-border-form cl-p am-fr">
                        <input type="text" class="am-form-field " v-model="key">
                        <span class="am-input-group-btn">
                            <button class="am-btn  am-btn-default am-btn-success tpl-table-list-field am-icon-search" v-on:click="search" type="button"></button>
                        </span>
                    </div>
                </div>


                <div class="am-u-sm-12">
                    <table class="am-table am-table-compact am-table-striped tpl-table-black " id="table-list">
                        <thead>
                            <tr>
                                <th>序号</th>
                                <th>账号</th>
                                <th>职业</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
@section scripts{
    <script src="~/Areas/Admin/Scripts/User/index.js"></script>
}



